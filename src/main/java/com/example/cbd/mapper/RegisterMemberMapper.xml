<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.cbd.mapper.RegisterMemberMapper">

    <insert id="insertMemberInfo" parameterType="com.example.cbd.entity.MemberRegistrationEntity">
        INSERT INTO
            USER1(NAME, PASSWORD)
        VALUES(
            #{userName},
            #{password})
    </insert>

</mapper>